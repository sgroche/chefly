<h1>Listing links</h1>
 
 <%= link_to 'New link', new_link_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Old Link</th>
    <th> New Link </th>
  </tr>
 
  <% @links.each do |link| %>
    <tr>
      <td><%= link.title %></td>
      <td><%= link.old_link %></td>
      <td><%= link.new_link %></td>
    </tr>
  <% end %>
  </table>
